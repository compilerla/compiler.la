{
    "name": "compiler.la",
    "dockerComposeFile": "docker-compose.yaml",
    "service": "site",
	"workspaceFolder": "/srv/jekyll",
    "postAttachCommand": [
        "/bin/bash",
        ".devcontainer/postAttach.sh"
      ],
    "settings": {
        "terminal.integrated.defaultProfile.linux": "bash",
        "terminal.integrated.profiles.linux": {
            "bash": {
                "path": "/bin/bash"
            }
        }
    },
    "extensions": [
        "eamodio.gitlens",
        "mhutchie.git-graph"
	]
}
